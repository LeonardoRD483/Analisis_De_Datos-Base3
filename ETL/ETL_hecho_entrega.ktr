    <sql>select mas.userid as alumno,ma.course as curso,ma.id as asignatura,
from_unixtime(ma.duedate) as fecha_de_entrega, DAYOFWEEK(from_unixtime(ma.duedate)) as dow_fecha_de_entrega,
from_unixtime(ma.cutoffdate) as fecha_cierre, DAYOFWEEK(from_unixtime(ma.cutoffdate )) as dow_fecha_cierre,
from_unixtime(mas.timemodified) as fecha_subida, DAYOFWEEK(from_unixtime(mas.timemodified )) as dow_fecha_subida
from mdl_assign_submission mas 
join mdl_assign ma on mas.`assignment` = ma.id</sql>
